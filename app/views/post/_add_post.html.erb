<%=form_for(@post, as: :post, url: post_index_path, html: { multipart: true }) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
    <%= f.label :image %>
    <%= f.file_field :image, class: 'form-control'%>
    <%= f.submit 'Upload Photo', class: 'btn btn-default' %>
<% end %>
